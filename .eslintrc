{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true,
  },
  "rules": {
    "no-console": [0],
    "react/display-name": [1],
    "react/prop-types": [1],
    "no-unused-vars": [1],
    "no-useless-escape": [1]
  }
}